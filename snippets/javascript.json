{
	".jasmine.js": {
        "describe block": {
            "prefix": "jjDesc",
            "body": [
                "describe('${1:Object_to_test}', function() {", 
                "\t${2}", 
                "});"
            ],
            "description": "Jasmine javascript describe block"
        },
        "it block": {
            "prefix": "jjIt",
            "body": [
                "it('${1:Description_of_the_single_test}', function() {",
                "\t${2}",
                "});"
            ],
            "description": "Jasmine javascript it function that contains a single test"
        },
        "beforeeach": {
            "prefix": "jjBeforeeach",
            "body": [
                "beforeEach(${1:function_name});${9}"
            ],
            "description": "Jasmine javascript beforeEach function"
        },
        "aftereach": {
            "prefix": "jjAftereach",
            "body": [
                "afterEach(${1:function_name});${9}"
            ],
            "description": "Jasmine javascript afterEach function"
        },
        "expectation": {
            "prefix": "jjExp",
            "body": [
                "expect(${1:item}).${2:to...}(${3:expected_value});${4}"
            ],
            "description": "Jasmine javascript expectation function"
        },
        "spyOn": {
            "prefix": "jjSpy",
            "body": [
                "spyOn(${1:module}, '${2:function_name}')${3:.and};"
            ]
        },
        "spyOn and expect": {
            "prefix": "jjSpyAndExp",
            "body": [
                "spyOn(${1:module}, '${2:function_name}')${3:.and};",
                "expect(${1:module}.${2:function_name}).toHaveBeenCalled${5}();${9}"
            ]
        }
	}
}